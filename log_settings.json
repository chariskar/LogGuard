{
    "LogLevels": {
        "INFO": {
            "number":1,
            "color": "green"
        },
        "DEBUG": {
            "number":2,
            "color": "blue"
        },
        "WARNING": {
            "number":3,
            "color": "orange"
        },
        "ERROR": {
            "number":4,
            "color": "red"
        },
        "CRITICAL": {
            "number":5,
            "color": "cherry-red"
        }
    },
    "Formats":{
        "NonContext": "[{timestamp}] [{level}] {message}  ",
        "Context":"[{timestamp}] [{level}] {message} {context}"
    },
    "Plugins":{
        "enabled": false ,
        "UsedPlugins": [
            "close",
            "create",
            "getname",
            "log"
        ],

        "PluginPath": {
            "close": "./Example-Plugins/Logic/Close.Plugin",
            "create": "./Example-Plugins/Logic/CreateFile.Plugin",
            "getname": "./Example-Plugins/Logic/GetName.Plugin",
            "log":"./Example-Plugins/Logic/Log.Plugin.ts"
        },

        "startup": {
            "enabled": false,
            "UsedPlugins": [
                "your-example-name"
            ],
            "Path":{
                "your-example-name":"./Example-Plugins/Function/YourExampleName.Plugin.ts"
            }
        }
    }
}
